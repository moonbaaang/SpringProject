<%
	String image = request.getParameter("image");
	String ocrResult = (String)request.getAttribute("ocrResult");
	JSONObject obj = new JSONObject(ocrResult);
	Object imsi = obj.get("images");
	JSONArray images = (JSONArray)imsi;
	for(Object one : images){
		JSONObject field = (JSONObject)((JSONObject)one).get("fields");
		
		out.print(field.get("inferText"));
	}
	

%>
